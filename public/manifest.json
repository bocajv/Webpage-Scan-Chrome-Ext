{
    "manifest_version": 3,
    "name": "Webpage Scanner",
    "version": "1.0",
    "description": "Scans a webpage and returns any data that might be of interest",
    "permissions": ["tabs", "scripting", "activeTab", "cookies"],
    "host_permissions": ["<all_urls>"],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png" 
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png" 
      }
  }
}
  